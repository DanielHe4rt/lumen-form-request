{
    "name": "danielhe4rt/lumenformrequest",
    "description": "Package to replicate Laravel FormRequest on Lumen Framework",
    "license": "license",
    "authors": [
        {
            "name": "Daniel Reis",
            "email": "hey@danielheart.dev",
            "homepage": "https://danielheart.dev"
        }
    ],
    "homepage": "https://github.com/danielhe4rt/lumenformrequest",
    "keywords": ["Laravel", "LumenFormRequest"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "danielhe4rt\\LumenFormRequest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "danielhe4rt\\LumenFormRequest\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "danielhe4rt\\LumenFormRequest\\LumenFormRequestServiceProvider"
            ],
            "aliases": {
                "LumenFormRequest": "danielhe4rt\\LumenFormRequest\\Facades\\LumenFormRequest"
            }
        }
    }
}
